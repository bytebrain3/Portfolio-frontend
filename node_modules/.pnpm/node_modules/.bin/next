#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/port/node_modules/.pnpm/next@13.4.20-canary.15_@babel+core@7.26.0_react-dom@18.2.0_react@18.3.1__react@18.3.1/node_modules/next/dist/bin/node_modules:/data/data/com.termux/files/home/port/node_modules/.pnpm/next@13.4.20-canary.15_@babel+core@7.26.0_react-dom@18.2.0_react@18.3.1__react@18.3.1/node_modules/next/dist/node_modules:/data/data/com.termux/files/home/port/node_modules/.pnpm/next@13.4.20-canary.15_@babel+core@7.26.0_react-dom@18.2.0_react@18.3.1__react@18.3.1/node_modules/next/node_modules:/data/data/com.termux/files/home/port/node_modules/.pnpm/next@13.4.20-canary.15_@babel+core@7.26.0_react-dom@18.2.0_react@18.3.1__react@18.3.1/node_modules:/data/data/com.termux/files/home/port/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/port/node_modules/.pnpm/next@13.4.20-canary.15_@babel+core@7.26.0_react-dom@18.2.0_react@18.3.1__react@18.3.1/node_modules/next/dist/bin/node_modules:/data/data/com.termux/files/home/port/node_modules/.pnpm/next@13.4.20-canary.15_@babel+core@7.26.0_react-dom@18.2.0_react@18.3.1__react@18.3.1/node_modules/next/dist/node_modules:/data/data/com.termux/files/home/port/node_modules/.pnpm/next@13.4.20-canary.15_@babel+core@7.26.0_react-dom@18.2.0_react@18.3.1__react@18.3.1/node_modules/next/node_modules:/data/data/com.termux/files/home/port/node_modules/.pnpm/next@13.4.20-canary.15_@babel+core@7.26.0_react-dom@18.2.0_react@18.3.1__react@18.3.1/node_modules:/data/data/com.termux/files/home/port/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
